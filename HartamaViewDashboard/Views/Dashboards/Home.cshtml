                    "dom": 'r<"table-responsive responsive"t><"bottom"<"col-sm-6"i><"col-sm-6"p>><"clear">',
                    "processing": false,
                    "shorting":false,
                    "serverSide": true,
                    "scrollCollapse": true,
                    "scrollX": false,
                    "autoWidth": false,
                    "ordering": false,
                    "pageLength": 20,
                    "ajax": {
                        "type": "post",
