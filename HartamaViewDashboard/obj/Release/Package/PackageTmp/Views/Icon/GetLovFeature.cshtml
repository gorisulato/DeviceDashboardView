@{
    Layout = null;
}


<div class="btn-group">
    <div class="input-group m-b">
        <input class="form-control m-b" type="text" placeholder="Search..." id="customSearchFeature">
        <span class="input-group-btn">
            <button class="btn btn-primary" type="button" id="frm-lov-btn-src-feature"><i class="fa fa-search"></i></button>
        </span>
    </div>
</div>
<table id="TIconLovFeature" class="table table-striped table-bordered table-hover dataTables-example">
    <thead>
        <tr>
            @*<th>ID Icon</th>*@
            <th>Icon Name</th>
            <th>Icon Class</th>
            <th>Preview</th>

        </tr>
    </thead>

    <tfoot>
    </tfoot>
</table>
<script>
        $(document).ready(function () {
            $.getScript("../Scripts/main/Icon.js", {
                cache: true
            }).done(
         function () {
             var param = {};
             param.attribute = {

                 "Info": "@Resources.Resource.LabelShowing",
                 "Page": "@Resources.Resource.LabelPage",
                 "Of": "@Resources.Resource.LabelOf",
                 "ZeroResult": "@Resources.Resource.LabelZeroResult",
                 "InfoEmpty": "@Resources.Resource.LabelInfoEmpty",
                 "Previous": "@Resources.Resource.LabelPrev",
                 "Next": "@Resources.Resource.LabelNext",
                 "Last": "@Resources.Resource.LabelLastPage",
                 "First": "@Resources.Resource.LabelFirstPage",
                 "To": "@Resources.Resource.LabelTo",
                 "Entries": "@Resources.Resource.LabelEntries",
             }
             IconLOVFeature.init(param);
         }
     )
        })
</script>

