                                    value.push({SensorID:rowsplited[1],value:rowsplited[2],date:rowsplited[3]})
                                }
                            }
                            
                           

                        }
                        
                        
                    }
                   // console.log(value)
                    
                    var valuesensor =[]
                    const unique1 = [...new Set(value.map(item => item.SensorID))];
                    // console.log($("#DashboardSensorList"));
                    
                   
                   
                    for (var i = 0; i < unique1.length; i++) {
                        var sortedchart = []
                        var xx = $("#DashboardSensorList").DataTable().rows(i).data()
                        
                        if (document.getElementById("chart"+ unique1[i]) == null) {
