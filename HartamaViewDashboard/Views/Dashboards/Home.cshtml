                success: function (result) {
                    //console.log(result.Result.length)
                    for (var x = 0; x < result.Result.length; x++) {

                        var resultsplit = result.Result[x].split("\n")
                       
                        for (var z = 0; z < resultsplit.length; z++) {

                            var rowsplited = resultsplit[z].split(",");
                          // console.log(rowsplited);
                           if (rowsplited[0] !="") {

