

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Technician Dashboards</title>
</head>
<body>
    <div>
        <div class="wrapper-md-expanded wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="col-md-12">
                    <div class="ibox float-e-margins">
                        <div class="de-ibox-wrapper">
                            <div class="ibox-title">
                                <h3>Device Selection</h3>
                            </div>
                            <div class="ibox-content">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <div class="form-horizontal">
                                            <div class="form-group" style="margin-bottom:0px">
                                                <label class="col-sm-3 col-lg-1 control-label">@Resources.Resource.labelDevice</label>
                                                <div class="col-sm-9 col-lg-5">
                                                    <div class="input-group">

                                                        @Html.TextBox("DeviceNameDashborad", "", new { @class = "date-model form-control required", @readonly = "" })
                                                        <span class="input-group-btn">
                                                            <button class="btn btn-outline btn-default" type="button" id="frm-btn-DeviceDashboard"><i class="fa fa-search"></i></button>
                                                        </span>
                                                        @Html.Hidden("DeviceIDDashboard")
                                                        @Html.Hidden("GapDashboard")
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">
                        <div class="de-ibox-wrapper">
                            <div class="ibox-title">
                                <h3><i class="fa fa-hdd-o"></i> Device Health Status </h3>
                            </div>
                            <div class="ibox-content">
                                <div id="content-chart-detail"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="ModalDataDeviceDashboard" class="modal inmodal " aria-hidden="true" role="dialog" tabindex="1" style="overflow-y: scroll; max-height:100%;  margin-top: 50px; margin-bottom:50px ; ">
            <div class="modal-dialog modal-lg">
                <div class="modal-content animated fadein ">
                    <div class="modal-header">
                        <h3>@Resources.Resource.labelDevice</h3>
                    </div>
                    <div class="modal-body" id="modal-Render-UbahMotorACCModal">
                        <div class="panel-body">
                            @{ Html.RenderPartial("~/Views/Dashboards/DeviceSearchDashboard.cshtml");  }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
  

    <script src="~/Scripts/c3.min.js"></script>
    <script src="~/Scripts/d3-5.8.2.min.js"></script>
    <script src="~/Scripts/Pusher.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $.getScript("../Scripts/main/Dashboard2.js", {
                cache: true
            }).done(
                function () {
                    var param = @ViewBag.Gap;
                    window.paramdo.arraychartdetail = new Array();
                    $("#GapDashboard").val(param)
                    Dashboard2.init(param);
                   
                }
            )
        });
        @*$(document).ready(function () {
            if ("@Session["IDUser"]" != null) {
                console.log(@ViewBag.Gap)

            }
            else {
                document.location = "../Login/Logout/"
            }



        })

        function Generate_Chart(Gap) {

            setInterval(function () {

                alert("Hello");
            }, parseInt(Gap));

        }*@
        



    </script>
</body>
</html>
