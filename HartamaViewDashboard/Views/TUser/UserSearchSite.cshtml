
<div class="row">
    <div class="col-lg-12">


        <div class="col-lg-8">
            <div class="form-inline">

                <div class="form-group">
                    <label for="Keyword" class="sr-only"> Keyword </label>
                    <input type="text" placeholder="Keyword" id="keywordUserSite" class="form-control">
                </div>

                <button class="btn btn-white" id="btnSearchUserSite">Cari</button>
                @Html.CheckBox("SelectAll")
                <label>Select All</label>
            </div>

        </div>

        <div class="col-lg-12">
            <div class="col-lg-12">

                <table id="UserSitetable" class="table table-striped table-bordered table-hover dataTables-example custom_table_scroll">
                    <thead>
                        <tr>

                            <th>@Resources.Resource.Name</th>
                            <th>@Resources.Resource.Address</th>
                           

                        </tr>
                    </thead>
                    <tfoot>
                    </tfoot>
                </table>
            </div>
        </div>

    </div>
</div>
